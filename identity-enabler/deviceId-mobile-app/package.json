{
    "name": "@zebra-iota-edge-sdk/device-id",
    "version": "0.1.0",
    "license": "MIT",
    "engines": {
        "node": ">=14",
        "npm": ">=7"
    },
    "exports": {
        "./package.json": "./package.json"
    },
    "scripts": {
        "test": "jest --silent",
        "test:watch": "npm run test -- --watchAll",
        "test:coverage": "jest --coverage --silent",
        "test:ci": "npm run test -- --ci --testLocationInResults --reporters=default --reporters=jest-junit",
        "build": "rollup -c",
        "build:dev": "rollup -c -w",
        "build:lint": "eslint src",
        "build:format": "prettier ./** --write",
        "start": "sirv public --single",
        "start:dev": "sirv public --single --dev --host --port 5003",
        "serve:capacitor": "run-p build:dev start:dev",
        "android:sync": "cap sync android && cap open android",
        "android:dev": "cp capacitor.config-dev.json capacitor.config.json && run-p serve:capacitor android:sync",
        "android": "cp capacitor.config-prod.json capacitor.config.json && run-s build android:sync"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.16.11",
        "@capacitor/cli": "^2.4.7",
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@rollup/plugin-typescript": "^8.2.1",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/svelte": "^3.0.3",
        "@types/jest": "^27.4.0",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.10.2",
        "babel-jest": "^27.4.6",
        "cross-env": "^7.0.3",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.23.2",
        "eslint-plugin-jsdoc": "^37.7.0",
        "eslint-plugin-svelte3": "^3.4.0",
        "eslint-plugin-unicorn": "40.1.0",
        "jest": "^27.4.7",
        "jest-css-modules-transform": "^4.3.0",
        "jest-junit": "^13.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "prettier-plugin-svelte": "^2.3.0",
        "rollup": "^2.46.0",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-string": "^3.0.0",
        "rollup-plugin-svelte": "^7.1.0",
        "rollup-plugin-terser": "^7.0.2",
        "svelte": "^3.38.0",
        "svelte-check": "^1.5.4",
        "svelte-jester": "^2.3.1",
        "svelte-loader": "^3.1.1",
        "svelte-preprocess": "^4.7.2",
        "ts-jest": "^27.1.3",
        "tslib": "^2.2.0",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "@capacitor/android": "^2.4.7",
        "@capacitor/core": "^2.4.7",
        "@iota/identity-wasm": "~0.4.0",
        "@rollup/plugin-json": "^4.1.0",
        "@zxing/library": "^0.18.6",
        "bwip-js": "^3.0.0",
        "capacitor-secure-storage-plugin": "^0.5.0",
        "hammerjs": "^2.0.8",
        "highlight.js": "^11.0.1",
        "markdown-it": "^12.1.0",
        "rollup-plugin-copy": "^3.4.0",
        "sirv-cli": "^1.0.11",
        "svelte-routing": "^1.6.0",
        "svelte-simple-modal": "^0.10.4",
        "uuid": "^8.3.2",
        "ws": "^7.5.3"
    }
}
